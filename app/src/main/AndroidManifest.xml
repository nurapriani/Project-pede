    <uses-feature
        android:name="android.software.leanback"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:logo="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pede"
        tools:targetApi="31">
        <activity
            android:name="com.pede.pede.BacaActivity5"
            android:exported="false" />
        <activity
            android:name="com.pede.pede.BacaActivity4"
            android:exported="false" />
        <activity
            android:name="com.pede.pede.BacaActivity3"
            android:exported="false" />
        <activity
            android:name="com.pede.pede.BacaActivity2"
            android:exported="false" />
        <activity
            android:name="com.pede.pede.BacaActivity1"
            android:exported="false" />
        <activity android:name="com.pede.pede.HomeActivity" />
        <activity android:name="com.pede.pede.OnBoardingActivity4" />
        <activity android:name="com.pede.pede.OnBoardingActivity3" />
        <activity android:name="com.pede.pede.OnBoardingActivity2" />
        <activity android:name="com.pede.pede.OnBoardingActivity" />
        <activity
            android:name="com.pede.pede.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>